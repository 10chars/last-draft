{
  "name": "last-draft",
  "version": "0.0.3",
  "description": "Last Draft... A Draft.js Editor using draft-js-plugins",
  "main": "lib/index.js",
  "scripts": {
    "test": "standard 'src/**/*.js' && tape test/index.js",
    "build": "rm -rf lib && babel src --out-dir lib --copy-files",
    "site": "npm run clean && NODE_ENV=prod webpack --config example/webpack.config.prod",
    "clean": "rm public/bundle.js && rm public/vendor.js || true",
    "dev": "NODE_ENV=dev webpack-dev-server --port 3000 --inline --hot --config example/webpack.config"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vacenz/last-draft"
  },
  "author": "Steven Iseki <stevenisekimartin@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "animateplus": "^1.4.0",
    "babel-core": "^6.7.6",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "babel-runtime": "^6.6.1",
    "express": "^4.13.4",
    "html-webpack-plugin": "^2.16.0",
    "react": ">=15.3.1",
    "react-dom": ">=15.3.1",
    "standard": "^8.6.0",
    "tape": "^4.6.3",
    "webpack": "2.1.0-beta.27",
    "webpack-dev-server": "2.1.0-beta.12"
  },
  "dependencies": {
    "draft-convert": "^1.3.1",
    "draft-js": "^0.9.1",
    "draft-js-export-html": "^0.5.2",
    "immutable": "~3.7.4",
    "linkify-it": "^2.0.2",
    "setimmediate": "^1.0.4",
    "styled-components": "^1.2.1",
    "tlds": "^1.182.0"
  },
  "keywords": [
    "last draft",
    "last-draft",
    "draft-js",
    "draft-js-plugins",
    "draftjs plugins",
    "draftjs",
    "editor",
    "richtext",
    "react"
  ]
}
